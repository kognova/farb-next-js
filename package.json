{
    "name": "farb-next-js-monorepo",
    "version": "1.0.0",
    "private": true,
    "workspaces": [
        "frontend",
        "backend"
    ],
    "scripts": {
        "dev:web": "yarn workspace frontend dev",
        "dev:api": "yarn workspace backend start:dev",
        "dev": "concurrently \"yarn dev:web\" \"yarn dev:api\"",
        "build:web": "yarn workspace frontend build",
        "build:api": "yarn workspace backend build",
        "build": "yarn build:web && yarn build:api",
        "start:web": "yarn workspace frontend start",
        "start:api": "yarn workspace backend start",
        "start": "concurrently \"start:web\" \"yarn start:api\""
    },
    "packageManager": "yarn@4.5.1",
    "devDependencies": {
        "concurrently": "^9.0.1"
    }
}
